<body>
<script>
		socket= new WebSocket('ws://30.28.176.149:41128');
		socket.onopen= function() {
		    socket.send('hello');
		};
		socket.onmessage= function(event) {
		    document.write("<p>"+event.data+"</p>");
		};
		document.write("<h1>This is a heading9999999999</h1>");
		document.write("<p>This is a paragraph</p>");
	</script>
